{
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "extensions.ignoreRecommendations": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.renderWhitespace": "none",
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.list.smoothScrolling": true,
  "workbench.colorTheme": "poimandres-noitalics-storm",
  "git.suggestSmartCommit": false,
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "chalice-icon-theme",
  "workbench.statusBar.visible": true,
  "autoHide.sideBarDelay": 50,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "terminal.integrated.gpuAcceleration": "off",
  "vim.neovimConfigPath": "/usr/bin/nvim",
  "typescript.tsserver.log": "off",
  "workbench.activityBar.visible": false,
  "editor.fontSize": 19,
  "editor.lineHeight": 1.618,
  "workbench.colorCustomizations": {
    "statusBar.background": "#282c34",
    "statusBar.noFolderBackground": "#282c34",
    "statusBar.debuggingBackground": "#282c34"
  },
  "editor.copyWithSyntaxHighlighting": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.minimap.renderCharacters": false,
  "editor.fontFamily": "Zed Mono regular",
  "editor.inlayHints.fontFamily": "Zed Mono regular",

  "vim.highlightedyank.duration": 500,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(250, 240, 170, 0.4)",
  "editor.guides.indentation": false,
  "editor.bracketPairColorization.enabled": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "window.zoomLevel": 1,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "apc.electron": {
    "titleBarStyle": "customButtonsOnHover"
    // "roundedCorners": false
  },
  "apc.header": {
    "height": 45,
    "fontSize": 14
  },
  "apc.listRow": {
    "height": 20,
    "fontSize": 11
  },
  "apc.menubar.compact": true,
  "apc.stylesheet": {
    "#workbench\\.parts\\.sidebar > .composite.title": "opacity: 0; pointer-events: none",
    ".title.tabs.title-border-bottom .tabs-container": "cursor: default; -webkit-app-region: no-drag;",
    ".editor-actions": "display: none;",
    ".monaco-list-rows": "background-color: transparent !important;",
    ".nosidebar .tabs": "padding-left: 80px;",
    ".scrollbar": " width: 10px !important",
    ".scrollbar .slider": "border-radius: 6px; width: calc(100% - 2px) !important;",
    ".editor .title": "background: transparent !important;",
    ".editor .title-border-bottom": "visibility: hidden !important;"
  },

  "apc.statusBar": {
    "position": "bottom",
    "height": 30,
    "fontSize": 16
  },
  "apc.font.family": "Zed Mono",
  "apc.monospace.font.family": "Zed Mono",
  "workbench.editor.showTabs": true,
  "breadcrumbs.enabled": false,
  "window.titleBarStyle": "native",
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  }
}
