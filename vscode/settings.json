{
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "extensions.ignoreRecommendations": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.renderWhitespace": "none",
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.list.smoothScrolling": true,
  "workbench.colorTheme": "Visual Studio Dark",
  "git.suggestSmartCommit": false,
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "city-lights-icons-vsc",
  "workbench.statusBar.visible": true,
  "autoHide.sideBarDelay": 50,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "terminal.integrated.gpuAcceleration": "off",
  "vim.neovimConfigPath": "/usr/bin/nvim",
  "typescript.tsserver.log": "off",
  "workbench.activityBar.visible": false,
  "editor.fontSize": 19,
  "workbench.colorCustomizations": {
    "statusBar.background": "#282c34",
    "statusBar.noFolderBackground": "#282c34",
    "statusBar.debuggingBackground": "#282c34"
  },
  "editor.copyWithSyntaxHighlighting": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.minimap.renderCharacters": false,
  "editor.fontFamily": "Satoshi regular",
  "vim.highlightedyank.duration": 500,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(250, 240, 170, 0.4)",
  "editor.guides.indentation": false,
  "editor.bracketPairColorization.enabled": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "window.zoomLevel": 1,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "apc.electron": {
    "thickFrame": true,
    "roundedCorners": false,
    "frame": false,
  },
  "apc.header": {
    "height": 20,
    "fontSize": 11
  },
  "apc.listRow": {
    "height": 20,
    "fontSize": 11
  },
  "apc.menubar.compact": true,
  "apc.statusBar": {
    "position": "bottom",
    "height": 30,
    "fontSize": 16
  },
  "workbench.editor.showTabs": true,
  "breadcrumbs.enabled": false,
  "window.titleBarStyle": "native",
}
