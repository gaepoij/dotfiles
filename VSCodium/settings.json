{
  "git.confirmSync": false,
  "git.autofetch": true,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "extensions.ignoreRecommendations": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.renderWhitespace": "none",
  "editor.cursorSmoothCaretAnimation": "on",
  "gitlens.gitCommands.closeOnFocusOut": true,
  "git.suggestSmartCommit": false,
  "workbench.iconTheme": "chalice-icon-theme",
  "workbench.statusBar.visible": true,
  "autoHide.sideBarDelay": 50,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "terminal.integrated.gpuAcceleration": "off",
  "vim.neovimConfigPath": "/usr/bin/nvim",
  "editor.tokenColorCustomizations": {
    // "functions": "#f0f0f0",
    // "numbers": "#f0f0f0",
    "comments": "#666666",
    // "keywords": "#a7a7a7",
    // "variables": "#f0f0f0",

    "textMateRules": [
      {
        "scope": "keyword",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "constant",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "punctuation.definition.block",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "meta.pun.undefined",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "constant.language.null",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "support.type.primitive",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "constant.numeric",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "punctuation.section.embedded",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": " keyword.operator.new",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "keyword.operator.ternary",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "punctuation.accessor.optional",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "meta.object-literal.key",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "meta.var.expr",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "meta.block",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "meta.block, meta.jsx.children",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "meta.brace.round",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },

      {
        "scope": "meta.type.declaration",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "meta.interface",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "meta.type.annotation",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "punctuation.terminator.statement",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "meta.tag",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "support.type.builtin ",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "meta.type.parameters",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "storage.type.function.arrow",
        "settings": {
          "foreground": "#ffffff"
        }
      },

      {
        "scope": "string",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },
      {
        "scope": "storage",
        "settings": {
          "foreground": "#a7a7a7"
        }
      },

      {
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "meta.var.expr.js storage",
        "settings": {
          "foreground": "#A7A7A7"
        }
      },
      {
        "scope": "meta.class.js storage",
        "settings": {
          "foreground": "#A7A7A7"
        }
      },
      {
        "scope": "meta.function.js storage",
        "settings": {
          "foreground": "#A7A7A7"
        }
      },
      {
        "scope": "meta.method.declaration.js storage",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "variable",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "entity.name.function",
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "scope": "support.class, entity.name.class, entity.name.type.class",
        "settings": {
          "foreground": "#ffffff"
        }
      }
    ]
  },

  "typescript.tsserver.log": "off",
  "editor.fontSize": 16,
  "workbench.colorCustomizations": {
    "scrollbarSlider.background": "#282525",
    "scrollbarSlider.hoverBackground": "#252121",
    "scrollbarSlider.activeBackground": "#252121",
    "editor.foreground": "#ffffff",
    "editor.selectionBackground": "#272424",
    "editorSuggestWidget.background": "#344255",

    // the first line in your gif is selected
    "editorSuggestWidget.selectedBackground": "#485669",

    // the letters you have typed  to bring up intellisense: 'bg' in your example
    "editorSuggestWidget.highlightForeground": "#97f4e2",

    // 'bg' in a selected entry
    "editorSuggestWidget.focusHighlightForeground": "#97f4e2",

    "editorSuggestWidget.foreground": "#fff", // the text color
    //Transparent border
    "tab.activeBorder": "#ff000000",
    "tab.activeBackground": "#252b37",
    "tab.inactiveBackground": "#0f0f0f",
    "quickInput.background": "#101010",
    "editor.background": "#0f0f0f",
    "editorLineNumber.foreground": "#f0f0f020",
    "editorLineNumber.activeForeground": "#f0f0f090"
  },
  "workbench.editor.tabCloseButton": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.minimap.renderCharacters": false,
  "editor.guides.indentation": false,
  "editor.bracketPairColorization.enabled": false,
  "git.mergeEditor": false,
  "tabnine.experimentalAutoImports": true,
  "editor.fontFamily": "'IBM Plex Mono'",
  "editor.lineHeight": 1.618,
  "workbench.fontAliasing": "antialiased",
  "editor.fontWeight": "400",
  "workbench.layoutControl.enabled": false,
  "editor.accessibilitySupport": "off",
  "gitlens.views.commitDetails.files.layout": "tree",
  "atlascode.bitbucket.enabled": true,
  "atlascode.jira.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "atlascode.bitbucket.pipelines.monitorEnabled": false,
  "atlascode.bitbucket.pipelines.explorerEnabled": false,
  "atlascode.bitbucket.statusbar.showUser": true,
  "atlascode.bitbucket.statusbar.showProduct": true,
  "atlascode.bitbucket.statusbar.enabled": true,
  "explorer.autoReveal": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": "true",
    "scminput": false
  },
  "apc.electron": {
    "titleBarStyle": "customButtonsOnHover",
    // "frame": true,
    "vibrancy": "sidebar",
    "visualEffectState": "active",
    "transparent": true
    // "backgroundColor": "rgb(15,15,15)"
  },
  "apc.header": {
    "height": 50,
    "fontSize": 14
  },
  "apc.listRow": {
    "height": 24,
    "fontSize": 14
  },
  "apc.menubar.compact": true,
  "apc.statusBar": {
    "position": "bottom",
    "height": 30,
    "fontSize": 16
  },
  "workbench.editor.showTabs": "multiple",
  "window.titleBarStyle": "native",
  "apc.imports": ["/Users/henrilahti/.vscode/resize-quickinput-widget.js"],
  "apc.stylesheet": {
    "#workbench\\.parts\\.sidebar > .composite.title": "opacity: 0; pointer-events: none",
    ".title.tabs.title-border-bottom .tabs-container": "cursor: default; -webkit-app-region: no-drag;",
    ".editor-actions": "display: none;",
    ".nosidebar .tabs": "padding-left: 80px;",
    ".scrollbar": " width: 10px !important",
    ".scrollbar .slider": "border-radius: 6px; width: calc(100% - 2px) !important;",
    ".suggest-widget": "border-radius: 8px !important; width: 600px !important;",
    ".tab": "border: 1px solid #1a2238 !important; border-top: 0px !important; border-bottom: 0px",
    ".tab:first-child": "border-left: 0px !important;",
    ".tab:last-child": "border-right: 0px !important;",
    ".tab.active": "border-bottom: 1px solid #111827 !important;"
  },
  "yaml.schemas": {
    "file:///Users/henrilahti/.vscode/extensions/atlassian.atlascode-3.0.7/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "editor.stickyScroll.enabled": true,
  "update.mode": "manual",
  "chaliceIcons.showArrows": true,
  "apc.font.family": "'IBM Plex Mono'",
  "apc.monospace.font.family": "'IBM Plex Mono'",
  "window.density.editorTabHeight": "compact",
  "editor.cursorBlinking": "blink",
  "workbench.list.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1",
    "FIG_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "workbench.activityBar.location": "hidden",
  "workbench.editor.tabActionCloseVisibility": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vscode-neovim.neovimClean": true,
  "workbench.colorTheme": "Monochrome Dark",
  "zenMode.hideLineNumbers": false,
  "window.commandCenter": false,
  "editor.minimap.enabled": false,
  "window.zoomLevel": 0.4,
  // "editor."
  "prisma.showPrismaDataPlatformNotification": false
  // "workbench.fontAliasing": "antialiased"
}
